{
  "displayName" : "Box",
  "group" : "OAuth",
  "templateUrl" : "/pages/Main/Main.html",
  "controllerUrl" : "/pages/Main/Main.js",
  "variablesUrl" : "/pages/Main/Main.variables.json",
  "iconUrl" : "/resources/images/imagelists/icon.png",
  "resources" : {
    "styles" : [ "/pages/Main/Main.css" ],
    "scripts" : [ ],
    "modules" : [ ]
  },
  "properties" : {
    "accesstoken" : {
      "type" : "string",
      "bindable" : "out-bound",
      "value" : "bind:Variables.OauthcallbackGetAccessToken.dataSet.value",
      "widget" : "string",
      "show" : false,
      "disabled" : false
    },
    "loginmode" : {
      "type" : "list",
      "displayName" : "Login Mode",
      "helpText" : "This property invokes the oAuth authorization on page load if set to auto.",
      "value" : "manual",
      "options" : [ "auto", "manual" ],
      "widget" : "list",
      "show" : true,
      "disabled" : false
    },
    "login" : {
      "type" : "method",
      "method" : "login"
    },
    "buttoncaption" : {
      "type" : "string",
      "displayName" : "Button Caption",
      "helpText" : "This property sets the caption onto the box login button.",
      "bindable" : "in-bound",
      "value" : "Login with Box",
      "widget" : "string",
      "show" : true,
      "disabled" : false
    },
    "buttonclass" : {
      "type" : "string",
      "displayName" : "Button Class",
      "helpText" : "This property sets the custom class onto the box login button",
      "bindable" : "in-bound",
      "widget" : "string",
      "show" : true,
      "disabled" : false
    },
    "callbackurl" : {
      "type" : "string",
      "displayName" : "Callback URL",
      "bindable" : "out-bound",
      "value" : "bind:Variables.OauthcallbackGetCallbackURL.dataSet.value",
      "widget" : "string",
      "show" : false,
      "disabled" : false
    },
    "loginsuccessmessage" : {
      "type" : "string",
      "displayName" : "Success Message",
      "helpText" : "This property will set the text for the toaster after successful authorization",
      "bindable" : "in-bound",
      "value" : "Login Successful!",
      "widget" : "string",
      "show" : true,
      "disabled" : false
    },
    "onLoginsuccess" : {
      "type" : "event",
      "widget" : "eventlist",
      "show" : true
    }
  }
}